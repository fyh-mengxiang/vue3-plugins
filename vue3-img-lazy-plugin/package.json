{
  "name": "vue3-img-lazy-plugin",
  "version": "1.0.0",
  "description": "A lightweight Vue 3 plugin for image lazy loading, supporting both Composition API and Option API",
  "main": "dist/img-lazy.cjs.js",
  "module": "dist/img-lazy.es.js",
  "browser": "dist/img-lazy.umd.js",
  "types": "dist/img-lazy.d.ts",
  "exports": {
    ".": {
      "import": "./dist/img-lazy.es.js",
      "require": "./dist/img-lazy.cjs.js",
      "browser": "./dist/img-lazy.umd.js",
      "types": "./dist/img-lazy.d.ts"
    }
  },
  "scripts": {
    "build": "vite build && node build-post.js",
    "dev": "vite",
    "preview": "vite preview"
  },
  "keywords": [
    "vue3",
    "vue",
    "lazyload",
    "directive",
    "image",
    "lazy-loading",
    "vue3-lazyload",
    "vue3-img-lazy-plugin"
  ],
  "author": "fyh-mengxiang",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@rollup/plugin-babel": "^6.1.0",
    "@vitejs/plugin-legacy": "^4.1.1",
    "vite": "^4.5.3"
  },
  "peerDependencies": {
    "vue": "^3.0.0 || ^2.7.0"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fyh-mengxiang/vue3-plugins.git"
  },
  "homepage": "https://github.com/fyh-mengxiang/vue3-plugins/tree/master/vue3-img-lazy-plugin#readme",
  "dependencies": {
    "core-js": "^3.47.0"
  }
}
